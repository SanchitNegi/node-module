        },
    }
});

    $("input[name='data[Order][type]']:radio").change(function() { 
        validator.resetForm();
        var result = $("input[name='data[Order][type]']:checked").val();
        if(result == 2){
            $(".float-right input").prop("disabled", false);
            $(".float-left input").prop("disabled", true);
            $(".float-left input[name='data[Order][type]']").prop("disabled", false);
            $(".float-left select").prop("disabled", true);
            $(".float-right select").prop("disabled", false);
        } else {
            $(".float-right input").prop("disabled", true);
            $(".float-left input").prop("disabled", false);
            $(".float-right input[name='data[Order][type]']").prop("disabled", false);
            $(".float-right select").prop("disabled", true);
            $(".float-left select").prop("disabled", false);
        }
    });
    
    var validator = $("#GuestDeliverOrdering").validate({
        rules: {
            "data[DeliveryAddress][name_on_bell]": {
