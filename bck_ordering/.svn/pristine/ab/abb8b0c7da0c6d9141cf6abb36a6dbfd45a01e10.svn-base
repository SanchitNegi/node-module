        $('.pickup_pickup_time_now').css('display','none');
        $('#PickUpPickupTimeNow').css('display','none');
        $('#PickUpPickupTime').css('display','block');
        $('#PickUpPickupDate').css('display','block');
    });
    $("#PickUpType0").on('click',function(){// To hide
