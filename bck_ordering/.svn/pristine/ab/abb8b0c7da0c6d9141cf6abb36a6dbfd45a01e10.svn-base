        $('#DeliveryPickupDate').css('display','none');
        
        $('.pickup_pickup_time').css('display','none');
        $('.pickup_pickup_time_now').css('display','block');
        $('#PickUpPickupTimeNow').css('display','block');
        $('#PickUpPickupTime').css('display','none');
        $('#PickUpPickupDate').css('display','none');
    });
    
    $('.date_select').datepicker({
        dateFormat: 'mm-dd-yy',
        minDate: 1,
    });

    $("#DeliveryType1").click(function(){ // To Show
        $('.delivery_pickup_time').css('display','block');
        $('#DeliveryPickupTime').css('display','block');
        $('#DeliveryPickupDate').css('display','block');
    });
